#ifndef CACHE
#define CACHE


struct mainThreadError : std::exception {
  const char* what()  const NOEXCEPT /*noexcept*/ override{
    return "failed to set up the proxy\n";
  }
};

struct childThreadError: std::exception{
    const char* what()  const NOEXCEPT /*noexcept*/ override{
    return "failed to set up the proxy\n";
  }
};

#endif
